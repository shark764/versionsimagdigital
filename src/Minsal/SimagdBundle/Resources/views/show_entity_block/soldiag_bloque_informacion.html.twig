{# empty Twig template #}

    <table class="table table-condensed">
        {% if solicitudDiagnosticoInfo is not null %}
            <tr>
                <th class="col-md-2">Solicitante:</th>
                <td class="col-md-4">
                    {{ solicitudDiagnosticoInfo.idEmpleado }}
                </td>
                <th class="col-md-2">Justificación:</th>
                <td class="col-md-4">
                    {{ solicitudDiagnosticoInfo.justificacion }}
                </td>
            </tr>
            <tr>
                <th class="col-md-2">Solicitado a:</th>
                <td class="col-md-4">
                    {{ solicitudDiagnosticoInfo.idEstablecimientoSolicitado }}
                </td>
                <th class="col-md-2">Próxima consulta médica:</th>
                <td class="col-md-4">
                    {{ solicitudDiagnosticoInfo.fechaProximaConsulta|format_date('EEEE, MMMM d, yyyy', 'es_ES') }}
                </td>
            </tr>
            <tr>
                <th class="col-md-2">Creada:</th>
                <td class="col-md-4">
                    {{ solicitudDiagnosticoInfo.fechaCreacion|format_date('EEEE, MMMM d, yyyy', 'es_ES') ~ ' ' ~ solicitudDiagnosticoInfo.fechaCreacion|date('H:i:s A') }}
                </td>
            </tr>
            <tr>
                <td colspan="4" class="col-md-4">
                    <button class='btn btn-primary' data-toggle='modal' href='#solicitudDiagnosticoFullData-showModalContainer' id='btn-ver-soldiag-total-info'>
                        <i class="fa fa-plus-circle"></i> Ver más
                    </button>
                </td>
            </tr>

        {% else %}
            <tr>
                <td class="col-md-12">
                    <div class="alert alert-info alert-dismissible" role="alert">
                        <button type="button"    class="close" data-dismiss="alert">
                            <span aria-hidden="true">&times;</span>
                            <span class="sr-only"> Cerrar </span>
                        </button>
                        <strong>Sin resultados!</strong> No se encontró registro.
                    </div>
                </td>
            </tr>
        {% endif %}
    </table>