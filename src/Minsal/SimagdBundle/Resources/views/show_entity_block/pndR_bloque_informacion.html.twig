{# empty Twig template #}

    <table class="table table-condensed">
        {% if pendienteRealizacionInfo is not null %}
            <tr>
                <th class="col-md-2">Ingresó a la lista:</th>
                <td class="col-md-4">
                    {{ pendienteRealizacionInfo.fechaIngresoLista|format_date('EEEE, MMMM d, yyyy', 'es_ES') ~ ' ' ~ pendienteRealizacionInfo.fechaIngresoLista|date('H:i:s A') }}
                </td>
                <th class="col-md-2">Realizar estudio en:</th>
                <td class="col-md-4">
                    {{ pendienteRealizacionInfo.idEstablecimiento }}
                </td>
            </tr>

        {% else %}
            <tr>
                <td class="col-md-12">
                    <div class="alert alert-info alert-dismissible" role="alert">
                        <button type="button"    class="close" data-dismiss="alert">
                            <span aria-hidden="true">&times;</span>
                            <span class="sr-only"> Cerrar </span>
                        </button>
                        <strong>Sin resultados!</strong> No se encontró registro.
                    </div>
                </td>
            </tr>
        {% endif %}
    </table>