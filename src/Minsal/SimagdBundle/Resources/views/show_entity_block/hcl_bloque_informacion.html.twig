{# empty Twig template #}

    <table class="table table-condensed">
        {% if historiaClinicaInfo is not null %}
            <tr>
                <th class="col-md-2">Fecha de consulta médica:</th>
                <td class="col-md-4">
                    {{ historiaClinicaInfo.fechaconsulta|format_date('EEEE, MMMM d, yyyy', 'es_ES') }}
                </td>
                <th class="col-md-2">Creado:</th>
                <td class="col-md-4">
                    {{ historiaClinicaInfo.fechahorareg|format_date('EEEE, MMMM d, yyyy', 'es_ES') ~ ' ' ~ historiaClinicaInfo.fechahorareg|date('H:i:s A') }}
                </td>
            </tr>
            <tr>
                <th class="col-md-2">Generada en:</th>
                <td class="col-md-4">
                    {{ historiaClinicaInfo.idestablecimiento }}
                </td>
                <th class="col-md-2">Procedencia:</th>
                <td class="col-md-4">
                    {{ historiaClinicaInfo.idsubservicio.idAreaModEstab.idAreaAtencion }}
                </td>
            </tr>
            <tr>
                <th class="col-md-2">Servicio que atendió:</th>
                <td class="col-md-4">
                    {{ historiaClinicaInfo.idsubservicio.idAtencion }}
                </td>
                <th class="col-md-2">Generó:</th>
                <td class="col-md-4">
                    {{ historiaClinicaInfo.idEmpleado }}
                </td>
            </tr>
            <tr>
                <td colspan="4" class="col-md-4">
                    <button class='btn btn-primary' data-toggle='modal' href='#historiaClinicaFullData-showModalContainer' id='btn-ver-hcl-total-info'>
                        <i class="fa fa-plus-circle"></i> Ver más
                    </button>
                </td>
            </tr>

        {% else %}
            <tr>
                <td class="col-md-12">
                    <div class="alert alert-info alert-dismissible" role="alert">
                        <button type="button" class="close" data-dismiss="alert">
                            <span aria-hidden="true">&times;</span>
                            <span class="sr-only"> Cerrar </span>
                        </button>
                        <strong>Sin resultados!</strong> No se encontró registro.
                    </div>
                </td>
            </tr>
        {% endif %}
    </table>