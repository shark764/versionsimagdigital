{# empty Twig template #}

    {% set _HCL_ALLOW_LIST_AND_VIEW = ( is_granted('ROLE_MINSAL_SIMAGD_ADMIN_SEC_HISTORIAL_CLINICO_LIST')
	    and is_granted('ROLE_MINSAL_SIMAGD_ADMIN_SEC_HISTORIAL_CLINICO_VIEW') ) or is_granted('ROLE_ADMIN') ? true : false %}
