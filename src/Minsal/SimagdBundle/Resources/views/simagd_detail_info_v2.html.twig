	    and is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_SOLICITUD_ESTUDIO_COMPLEMENTARIO_VIEW') ) or is_granted('ROLE_ADMIN') ? true : false %}
    {% set _SOLDIAG_ALLOW_LIST_AND_VIEW = ( is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_SOLICITUD_DIAGNOSTICO_LIST')
	    and is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_SOLICITUD_DIAGNOSTICO_VIEW') ) or is_granted('ROLE_ADMIN') ? true : false %}
    {% set _LCT_ALLOW_LIST_AND_VIEW = ( is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_LECTURA_LIST')
	    and is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_LECTURA_VIEW') ) or is_granted('ROLE_ADMIN') ? true : false %}
    {% set _DIAG_ALLOW_LIST_AND_VIEW = ( is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_DIAGNOSTICO_LIST')
