    {% set _PRZ_ALLOW_LIST_AND_VIEW = ( is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_PROCEDIMIENTO_REALIZADO_LIST')
	    and is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_PROCEDIMIENTO_REALIZADO_VIEW') ) or is_granted('ROLE_ADMIN') ? true : false %}
