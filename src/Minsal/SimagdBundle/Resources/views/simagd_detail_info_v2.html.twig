	    and is_granted('ROLE_MINSAL_SIMAGD_ADMIN_IMG_NOTA_DIAGNOSTICO_VIEW') ) or is_granted('ROLE_ADMIN') ? true : false %}

    <div class="col-xs-9">{# usar PILLS en lugar de tabs en simagd-info   http://api.jquery.com/jquery.param/ #}
        <!-- Tab panes -->
        <div class="tab-content">
	    <div class="tab-pane fade in active" id="pacienteInfo" >
		{% include 'MinsalSimagdBundle:show_entity_block:pct_bloque_informacion_v2.html.twig' %}
	    </div>
            {% if _HCL_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="historiaClinicaInfo" >
                    {% include 'MinsalSimagdBundle:show_entity_block:hcl_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
            {% if _PRC_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="preinscripcionInfo" data-refresh-url="{{ path('simagd_solicitud_estudio_listarSolicitudesEstudioSummary') }}" >
                    {% include 'MinsalSimagdBundle:show_entity_block:prc_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
            {% if _CIT_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="citaInfo" >
                    {% include 'MinsalSimagdBundle:show_entity_block:cit_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
            {% if _PRZ_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="procedimientoRealizadoInfo" >
                    {% include 'MinsalSimagdBundle:show_entity_block:prz_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
            {% if _SOLCMPL_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="solicitudComplementarioInfo" >
                    {% include 'MinsalSimagdBundle:show_entity_block:solcmpl_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
            {% if _SOLDIAG_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="solicitudDiagnosticoInfo" >
                    {% include 'MinsalSimagdBundle:show_entity_block:soldiag_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
            {% if _LCT_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="lecturaInfo" >
                    {% include 'MinsalSimagdBundle:show_entity_block:lct_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
            {% if _DIAG_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="diagnosticoInfo" >
                    {% include 'MinsalSimagdBundle:show_entity_block:diag_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
            {% if _NOTDIAG_ALLOW_LIST_AND_VIEW == true %}
                <div class="tab-pane fade" id="notaDiagInfo" >
                    {% include 'MinsalSimagdBundle:show_entity_block:notdiag_bloque_informacion_v2.html.twig' %}
                </div>
            {% endif %}
        </div>
    </div>

    <div class="col-xs-3"> <!-- required for floating -->
        <!-- Nav tabs -->
        <ul class="nav nav-tabs tabs-right">
            <li class="active"><a   href="#pacienteInfo" data-toggle="pill"><span class="glyphicon glyphicon-user"></span> Paciente</a></li>
            {% if _HCL_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#historiaClinicaInfo" data-toggle="pill"><span class="glyphicon glyphicon-list-alt"></span> Historia clínica</a></li>
            {% endif %}
            {% if _PRC_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#preinscripcionInfo" data-toggle="pill"><span class="glyphicon glyphicon-inbox"></span> Solicitudes</a></li>
            {% endif %}
            {% if _CIT_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#citaInfo" data-toggle="pill"><span class="glyphicon glyphicon-time"></span> Citas</a></li>
            {% endif %}
            {% if _PRZ_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#procedimientoRealizadoInfo" data-toggle="pill"><span class="glyphicon glyphicon-eye-open"></span> Exámenes</a></li>
            {% endif %}
            {% if _SOLCMPL_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#solicitudComplementarioInfo" data-toggle="pill"><span class="glyphicon glyphicon-repeat"></span> Solicitudes de estudio complementario</a></li>
            {% endif %}
            {% if _SOLDIAG_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#solicitudDiagnosticoInfo" data-toggle="pill"><span class="glyphicon glyphicon-inbox"></span> Solicitudes de diagnóstico</a></li>
            {% endif %}
            {% if _LCT_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#lecturaInfo" data-toggle="pill"><span class="glyphicon glyphicon-book"></span> Interpretación</a></li>
            {% endif %}
            {% if _DIAG_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#diagnosticoInfo" data-toggle="pill"><span class="glyphicon glyphicon-floppy-saved"></span> Diagnóstico</a></li>
            {% endif %}
            {% if _NOTDIAG_ALLOW_LIST_AND_VIEW == true %}
                <li><a   href="#notaDiagInfo" data-toggle="pill"><span class="glyphicon glyphicon-comment"></span> Notas agregadas <span class="badge"></span></a></li>
            {% endif %}
        </ul>
    </div>

    {# muestra de información completa #}
    {% if pacienteInfo is not null %}
        {% include 'MinsalSimagdBundle:show_container_fullEntity:pct_total_info_modal_v2.html.twig' %}     {# paciente #}
