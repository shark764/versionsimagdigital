    {% include 'MinsalSimagdBundle:RyxSolicitudEstudioAdmin:prc_actionEvents.html.twig' %}
    {% include 'MinsalSimagdBundle:RyxSolicitudEstudioAdmin:prc_emergency_actionEvents.html.twig' %}
    {% include 'MinsalSimagdBundle:RyxSolicitudDiagnosticoPostEstudioAdmin:soldiag_actionEvents.html.twig' %}
    {% include 'MinsalSimagdBundle:RyxSolicitudEstudioComplementarioAdmin:solcmpl_actionEvents.html.twig' %}
    <!--
    //////////////////////////////////////////////////////////////
    //////// END --| SCRIPT FUNCTIONS
    //////////////////////////////////////////////////////////////
     -->

