{# empty Twig template #}

{% extends 'MinsalSimagdBundle:CRUD:base_list.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}



{% block sonata_page_content_header %} {% endblock sonata_page_content_header %}

{% block sonata_breadcrumb %} {% endblock sonata_breadcrumb %}{# #}

{% block actions %} {% endblock %}{# #}

{% block tab_menu %} {% endblock %}{# #}



{% block content %}
    {{ parent() }}

    <div class=" {#col-lg-10 col-md-10 col-sm-10#} col-lg-12 col-md-12 col-sm-12 ">
        <div class="box box-drop-outside-shadow {#box-dark-theme#} box-primary-v4">
            <div class="box-body">
                <div class="row">
                    {% include 'MinsalSimagdBundle:Menu:panel_top_menu.html.twig' %} {#ignore missing with {'id': 'patient_modal_search_'}#}
                </div>
            </div>
        </div>
    </div>

    <div class=" col-lg-8 col-md-8 col-sm-8 col-xs-offset-2 ">
        <div class="box box-drop-outside-shadow {#box-dark-theme#} box-primary-v4">
            <div class="box-body">

                <!-- WORKLIST MENU -->
                <div class="col-lg-10 col-md-10 col-sm-10 nav-pills-darkblue ">
                    <ul class="nav nav-pills nav-justified" id="_request_dashboard_display_items">
                        <li role="presentation" class="dropdown active">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                                <span class="glyphicon glyphicon-inbox"></span> Solicitudes (est.) <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu highlight-success-dropdown-menu" role="menu">
                                <li class="active"><a href="javascript:void(0)" onclick="generate(this, 'study_request'); return false;" data-toggle="pill"> <span class="glyphicon glyphicon-inbox"></span> Solicitudes</a></li>
                                <li><a href="javascript:void(0)" onclick="generate(this, 'emergency_study_request', 1); return false;" data-toggle="pill"> <span class="glyphicon glyphicon-send"></span> Solicitudes (Emerg)</a></li>
                            </ul>
                        </li>
                        <li role="presentation" ><a href="javascript:void(0)" onclick="generate(this, 'diagnostic_request'); return false;" data-toggle="pill"> <span class="glyphicon glyphicon-adjust"></span> Solicitudes (diag)</a></li>
                        <li role="presentation" ><a href="javascript:void(0)" onclick="generate(this, 'further_study_request'); return false;" data-toggle="pill"> <span class="glyphicon glyphicon-inbox"></span> Solicitudes (adic.)</a></li>
                    </ul>
                </div>

                <!-- DISPLAY TYPE -->
                <ul class="list-unstyled list-inline input-sm " id="_request_dashboard_display_type" style="margin-top: 3px; margin-bottom: 6px;">
                    <li>
                        <label class="">
                            <input type="radio" checked="checked" value="list" name="_request_dashboard_display_type" id="_request_dashboard_display_type_0">
                            <span class="glyphicon glyphicon-align-justify" {# style="margin-left: 3px;" #}></span>
                        </label>
                    </li>
                    <li>
                        <label class="">
                            <input type="radio" {#checked="checked"#} value="compact" name="_request_dashboard_display_type" id="_request_dashboard_display_type_1">
                            <span class="glyphicon glyphicon-tasks" {# style="margin-left: 3px;" #}></span>
                        </label>
                    </li>
                    <li>
                        <label class="">
                            <input type="radio" {#checked="checked"#} value="detail" name="_request_dashboard_display_type" id="_request_dashboard_display_type_2">
                            <span class="glyphicon glyphicon-folder-open" {# style="margin-left: 3px;" #}></span>
                        </label>
                    </li>
                </ul>

            </div>
        </div>
    </div>



    <div class=" col-lg-12 col-md-12 col-sm-12 ">
        <div class="jumbotron" style="background-color: #284858;">
            <div class="container">
                <h2><strong><span class="text-emergency" id="_request_dashboard_jumbotron_title"> Listado de Solicitudes de estudio / procedimiento radiológico </span></strong></h2>
                <div class="btn-toolbar" role="toolbar" aria-label="..." id="_request_dashboard_jumbotron_btn_toolbar">
                    <div class="btn-group" role="group">
                        <a class="btn btn-highlighted" role="button" href="javascript:void(0)" onclick="generate(this, 'study_request_today'); return false;" title="generar tabla" >
                            Registros del día
                        </a>
                    </div>
                    <div class="btn-group" role="group">
                        <a class="btn btn-highlighted" role="button" href="javascript:void(0)" onclick="generate(this, 'study_request_week'); return false;" title="generar tabla" >
                            Registros de la semana
                        </a>
                    </div>
                    <div class="btn-group" role="group">
                        <a class="btn btn-highlighted" role="button" href="javascript:void(0)" onclick="generate(this, 'study_request_month'); return false;" title="generar tabla" >
                            Registros del mes
                        </a>
                    </div>
                    <div class="btn-group" role="group">
                        <a class="btn btn-highlighted" role="button" href="javascript:void(0)" onclick="generate(this, 'study_request'); return false;" title="generar tabla" >
                            Todos los registros
                        </a>
                    </div>
                </div>
                <p></p>
                <div class="btn-toolbar" role="toolbar" aria-label="...">
                    <div class="btn-group" role="group">
                        <a class="btn btn-emergency" id="_new_external_patient" name="_new_external_patient"><span class="glyphicon glyphicon-plus-sign"></span> Crear externo</a>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <div class="tab-pane {#fade#} in active" id="tab_list__" style="min-height: 768px;" >

        <!--
        //////////////////////////////////////////////////////////////
        //////// TABLE
        //////////////////////////////////////////////////////////////
         -->
        <!-- table for any result -->
        <div class=" {#col-lg-10 col-md-10 col-sm-10#} col-lg-12 col-md-12 col-sm-12 {#col-lg-8 col-md-8 col-sm-8 col-xs-offset-2#} " style="min-height: 768px;">
            <div class="box box-drop-outside-shadow {#box-dark-theme#} box-primary-v4">
                <div class="box-body">
                    <div class="row">
                        <table class="table table-condensed" id="ryx_target_bstable" name="ryx_target_bstable" {#data-toggle="table" #}>
                            <thead></thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- end table -->


        <!--
        //////////////////////////////////////////////////////////////
        //////// END --| TABLE
        //////////////////////////////////////////////////////////////
         -->

    </div>

    {#<div class=" col-lg-2 col-md-2 col-sm-2 ">

        <!-- vertical pills -->
        <div class=" nav-pills-primary-v4 ">
            <ul class="nav nav-pills nav-stacked " id="_stacked_request_dashboard_display_items">
                <li role="presentation" class="active"><a href="javascript:void(0)" onclick="generate(this, 'study_request'); return false;" data-toggle="pill"> <span class="glyphicon glyphicon-inbox"></span> SOLICITUDES</a></li>
                <li role="presentation" ><a href="javascript:void(0)" onclick="generate(this, 'emergency_study_request', 1); return false;" data-toggle="pill"> <span class="glyphicon glyphicon-send"></span> SOLICITUDES (EMERG)</a></li>
                <li role="presentation" ><a href="javascript:void(0)" onclick="generate(this, 'diagnostic_request'); return false;" data-toggle="pill"> <span class="glyphicon glyphicon-adjust"></span> SOLICITUDES (DIAG)</a></li>
                <li role="presentation" ><a href="javascript:void(0)" onclick="generate(this, 'further_study_request'); return false;" data-toggle="pill"> <span class="glyphicon glyphicon-inbox"></span> SOLICITUDES (ADIC.)</a></li>
            </ul>
        </div>

    </div>#}

