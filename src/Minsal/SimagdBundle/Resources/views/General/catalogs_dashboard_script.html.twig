{% autoescape false %}

    <script type="text/javascript">
        var _fn_show_object_detail = function(el, slug, id) {
            window.alert(slug + '\n' + id);
        }

        var generate = function(el, slug, emrg/* = 0, type = 'list'*/) {
			if (emrg === undefined) {
		        emrg = 0;
		    }
            ////////
            var $table = jQuery('#ryx_target_bstable');
            ////////
            $table.bootstrapTable('showLoading');

            $url = null;
			switch (slug) {
				case 'radiological_examinations':
					$url = 'simagd_proyeccion_generateTable';
					break;
				case 'local_radiological_examinations':
					$url = 'simagd_proyeccion_establecimiento_generateTable';
					break;
				case 'materials':
					$url = 'simagd_material_generateTable';
					break;
				case 'local_materials':
					$url = 'simagd_material_local_generateTable';
					break;
				case 'pacs_server_connections':
					$url = 'simagd_pacs_generateTable';
					break;
				case 'radiological_diagnostic_patterns':
					$url = 'simagd_patron_diagnostico_generateTable';
					break;
				default:
					break;
			}

            var type = jQuery('input[name=_catalogs_dashboard_display_type]:checked').val();

            $.ajax({
                type: 'POST',
                dataType: 'json',
                url: Routing.generate($url),
                data: {
                    _xml_http_request: true,
                    id: 'id',
                    slug: slug,
                    type: type,
                    emrg: emrg,
                },
                success: function(response) {
                    ////////
                    $table
                        .bootstrapTable('destroy')
                        .bootstrapTable(response.options);
                    ////////
                },
                error: function(err) {
                    console.log(err.error, err.responseText);
                }
            });
        }

        jQuery(document).ready(function() {
            /*
             *
             * @type type
             * Get Table definition
             */

            //////
            var $table = jQuery('#ryx_target_bstable');
            ////////
            // $table.bootstrapTable('showLoading');

            ////////
            var DEFAULT_TABLE_OPTIONS = JSON.parse('{{ DEFAULT_TABLE_OPTIONS|json_encode|e('js')|raw }}');

            $table
                .bootstrapTable('destroy')
                .bootstrapTable(DEFAULT_TABLE_OPTIONS);
            ////////


            //////////////////////////////////////////////////////////////////////////
            //////// change content type - worklist
            //////////////////////////////////////////////////////////////////////////
            var __DOM__worklistDisplayType = jQuery('input[name=_catalogs_dashboard_display_type]');

            __DOM__worklistDisplayType.on('ifChecked', function(e) {

                        jQuery('#_catalogs_dashboard_display_items li.active').find('a').triggerHandler('click');

            });
            //////////////////////////////////////////////////////////////////////////
            //////////////////////////////////////////////////////////////////////////


            // jQuery('#_switch_display').iCheck('destroy').bootstrapSwitch();

        });

        (function ($) {
            /*
             *
             * @type Array
             * Global variables for plugin
             */
            window.__FUNCTIONS_CALL__ = {};   // --| build data collection

            /*
             * Collection of functions / fc options
             */
            __FUNCTIONS_CALL__.functions = {
                onContextMenuItem: function(row, $el) {
                    /////////////////////////////////////////////////////
                    //////// RADIOLOGICAL EXAMINATIONS - CONTEXT MENU
                    /////////////////////////////////////////////////////
                    if($el.data('item') === 'radiologicalexaminations_show')
                    {
                        var url = Routing.generate('simagd_proyeccion_show', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'radiologicalexaminations_edit')
                    {
                        var url = Routing.generate('simagd_proyeccion_edit', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'radiologicalexaminations_delete')
                    {
                        var url = Routing.generate('simagd_proyeccion_delete', {id: row.id, confirmation: false});
                    }
                    else if($el.data('item') === 'radiologicalexaminations_create')
                    {
                        var url = Routing.generate('simagd_proyeccion_create', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'radiologicalexaminations_addtolocal')
                    {
                        var url = Routing.generate('simagd_proyeccion_create', {id: row.id, confirmation: false});
                    }

                    /////////////////////////////////////////////////////
                    //////// LOCAL RADIOLOGICAL EXAMINATIONS - CONTEXT MENU
                    /////////////////////////////////////////////////////
                    else if($el.data('item') === 'localradiologicalexaminations_show')
                    {
                        var url = Routing.generate('simagd_proyeccion_establecimiento_show', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'localradiologicalexaminations_edit')
                    {
                        var url = Routing.generate('simagd_proyeccion_establecimiento_edit', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'localradiologicalexaminations_delete')
                    {
                        var url = Routing.generate('simagd_proyeccion_establecimiento_delete', {id: row.id, confirmation: false});
                    }
                    else if($el.data('item') === 'localradiologicalexaminations_create')
                    {
                        var url = Routing.generate('simagd_proyeccion_establecimiento_create', {id: row.id, mode: 'standard'});
                    }

                    /////////////////////////////////////////////////////
                    //////// MATERIALS - CONTEXT MENU
                    /////////////////////////////////////////////////////
                    else if($el.data('item') === 'materials_show')
                    {
                        var url = Routing.generate('simagd_material_show', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'materials_edit')
                    {
                        var url = Routing.generate('simagd_material_edit', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'materials_delete')
                    {
                        var url = Routing.generate('simagd_material_delete', {id: row.id, confirmation: false});
                    }
                    else if($el.data('item') === 'materials_create')
                    {
                        var url = Routing.generate('simagd_material_create', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'materials_addtolocal')
                    {
                        var url = Routing.generate('simagd_material_create', {id: row.id, confirmation: false});
                    }

                    /////////////////////////////////////////////////////
                    //////// LOCAL MATERIALS - CONTEXT MENU
                    /////////////////////////////////////////////////////
                    else if($el.data('item') === 'localmaterials_show')
                    {
                        var url = Routing.generate('simagd_material_local_show', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'localmaterials_edit')
                    {
                        var url = Routing.generate('simagd_material_local_edit', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'localmaterials_delete')
                    {
                        var url = Routing.generate('simagd_material_local_delete', {id: row.id, confirmation: false});
                    }
                    else if($el.data('item') === 'localmaterials_create')
                    {
                        var url = Routing.generate('simagd_material_local_create', {id: row.id, mode: 'standard'});
                    }

                    /////////////////////////////////////////////////////
                    //////// PACS SERVER CONNECTIONS - CONTEXT MENU
                    /////////////////////////////////////////////////////
                    else if($el.data('item') === 'pacsserverconnections_show')
                    {
                        var url = Routing.generate('simagd_pacs_show', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'pacsserverconnections_edit')
                    {
                        var url = Routing.generate('simagd_pacs_edit', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'pacsserverconnections_delete')
                    {
                        var url = Routing.generate('simagd_pacs_delete', {id: row.id, confirmation: false});
                    }
                    else if($el.data('item') === 'pacsserverconnections_create')
                    {
                        var url = Routing.generate('simagd_pacs_edit', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'pacsserverconnections_testconnection')
                    {
                        var url = Routing.generate('simagd_pacs_testconnection', {id: row.id, confirmation: false});
                    }

                    /////////////////////////////////////////////////////
                    //////// RADIOLOGICAL DIAGNOSTIC PATTERNS - CONTEXT MENU
                    /////////////////////////////////////////////////////
                    else if($el.data('item') === 'radiologicaldiagnosticpatterns_show')
                    {
                        var url = Routing.generate('simagd_patron_diagnostico_show', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'radiologicaldiagnosticpatterns_edit')
                    {
                        var url = Routing.generate('simagd_patron_diagnostico_edit', {id: row.id, mode: 'standard'});
                    }
                    else if($el.data('item') === 'radiologicaldiagnosticpatterns_delete')
                    {
                        var url = Routing.generate('simagd_patron_diagnostico_delete', {id: row.id, confirmation: false});
                    }
                    else if($el.data('item') === 'radiologicaldiagnosticpatterns_create')
                    {
                        var url = Routing.generate('simagd_patron_diagnostico_create', {id: row.id, mode: 'standard'});
                    }
                    // end
                },
            };  // --| build function object

        }(jQuery));

    </script>

