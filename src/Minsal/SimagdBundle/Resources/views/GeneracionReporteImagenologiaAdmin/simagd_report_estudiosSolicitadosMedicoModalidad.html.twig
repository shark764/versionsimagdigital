{# empty Twig template #}


    <form id="formEstudiosSolicitadosMedicoModalidad" method="post" class="form-horizontal" >

        <div class="form-group col-sm-12">
            <label class="col-sm-2 control-label">Fecha Inicio</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fechaIni_7" name="fechaIni_7" readonly="readonly" />
            </div>
            <label class="col-sm-2 control-label">Fecha final</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fechaFin_7" name="fechaFin_7" readonly="readonly" />
            </div>
        </div>
        <div class="form-group col-sm-12">
            <label class="col-sm-2 control-label">Modalidad</label>
            <div class="col-sm-6">
                <select id="modalidadPreinscrita_7" name="modalidadPreinscrita_7" class="form-control">
                    <option value=""></option>
                    <optgroup label="ImagenologÃ­a">
                    {% for modalidadPrc in modalidadesPrc %}
                        <option value="{{ modalidadPrc.id }}">{{ modalidadPrc }}</option>
                    {% endfor %}
                    </optgroup>
                </select>
            </div>
        </div>
        <div class="form-group col-sm-12">
            <label class="col-sm-2 control-label">Solicitante</label>
            <div class="col-sm-6">
                <select id="preinscriptorEstudio_7" name="preinscriptorEstudio_7" class="form-control">
                    <option value=""></option>
                    {% for tipo in tiposEmpleado if tipo.codigo == 'MED' %}
                        <optgroup label="{{ tipo }}">
                        {% for preinscriptor in preinscriptores if preinscriptor.idTipoEmpleado is not null and preinscriptor.idTipoEmpleado.id == tipo.id %}
                            <option value="{{ preinscriptor.id }}">{{ preinscriptor }}</option>
                        {% endfor %}
                        </optgroup>
                    {% endfor %}
                    {% for preinscriptor in preinscriptores if preinscriptor.idTipoEmpleado is null %}
                        <option value="{{ preinscriptor.id }}">{{ preinscriptor }}</option>
                    {% endfor %}
                </select>
            </div>
        </div>
        
        <input type="hidden" class="form-control" name="report_selector" value="7" />

    </form>