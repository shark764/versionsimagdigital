{# empty Twig template #}


    <form id="formPacientesAtendidosMedico" method="post" class="form-horizontal" >

        <div class="form-group col-sm-12">
            <label class="col-sm-2 control-label">Fecha Inicio</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fechaIni_2" name="fechaIni_2" readonly="readonly" />
            </div>
            <label class="col-sm-2 control-label">Fecha final</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fechaFin_2" name="fechaFin_2" readonly="readonly" />
            </div>
        </div>
        <div class="form-group col-sm-12">
            <label class="col-sm-2 control-label">Tecn√≥logo</label>
            <div class="col-sm-6">
                <select id="tecnologoRealizo_2" name="tecnologoRealizo_2" class="form-control">
                    <option value=""></option>
                    {% for tipo in tiposEmpleado if tipo.codigo == 'MED' or tipo.codigo == 'TRY' %}
                        <optgroup label="{{ tipo }}">
                        {% for tecnologo in tecnologos if tecnologo.idTipoEmpleado is not null and tecnologo.idTipoEmpleado.id == tipo.id %}
                            <option value="{{ tecnologo.id }}">{{ tecnologo }}</option>
                        {% endfor %}
                        </optgroup>
                    {% endfor %}
                    {% for tecnologo in tecnologos if tecnologo.idTipoEmpleado is null %}
                        <option value="{{ tecnologo.id }}">{{ tecnologo }}</option>
                    {% endfor %}
                </select>
            </div>
        </div>
        
        <input type="hidden" class="form-control" name="report_selector" value="2" />

    </form>