    <script type="text/javascript">
        jQuery(document).ready(function() {

            /** Validación de formulario de envío de parametros */
            $("form[id='formExamenesRealizados']").bootstrapValidator({
                //excluded: [':disabled'],
                feedbackIcons: {
                    valid: 'glyphicon glyphicon-ok',
                    invalid: 'glyphicon glyphicon-remove',
                    validating: 'glyphicon glyphicon-refresh'
                },
                fields: {
                    fechaIni_3: {
                        message: 'Fecha es inválida',
                        validators: {
                            notEmpty: {
                                message: 'Fecha de inicio es requerida'
                            },
                        }
                    },
                    fechaFin_3: {
                        message: 'Fecha es inválida',
                        validators: {
                            callback: {
                                message: 'No puede seleccionar esta fecha',
                                callback: function(value, validator) {
                                    /** begin callback function */
                                            
                                    if (value === '') { return true; }
                                    
                                    var dateIniAsObject = $("#fechaIni_3").datepicker( 'getDate' );
                                    var dateFinAsObject = $("#fechaFin_3").datepicker( 'getDate' );
                                    
		                    if ( dateIniAsObject && dateFinAsObject && dateIniAsObject > dateFinAsObject ) {
		                        return {
		                            valid: false,
		                            message: 'Fecha inicial no puede ser mayor a final'
		                        };
		                    }

	                            return true;
				    
                                    /** end callback function */
                                }
                            },
                        }
                    },
                }
            })
            .on('success.form.bv', function(e) {
                // Prevent form submission
                e.preventDefault();

                // Get the form instance
                var $form = $(e.target);

                // Get the BootstrapValidator instance
                var bv = $form.data('bootstrapValidator');
                
                var $formParam_object = $form.formParams();
		var opcionesGeneralesObject = {
			report_format: $("select[id='report_format']").val(),
			limite_resultados: $("select[id='limite_resultados']").val()
		};
		$.extend( $formParam_object, opcionesGeneralesObject );

		console.log($formParam_object );
                
                var url = Routing.generate('simagd_generacion_reporte_generarReporteImagenologico', $formParam_object);
                window.open(url, '_blank');

                console.log(url );
            });
            
        });
    </script>
    
    
