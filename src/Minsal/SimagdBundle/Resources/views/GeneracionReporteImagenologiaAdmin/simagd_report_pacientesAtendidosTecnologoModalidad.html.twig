{# empty Twig template #}


    <form id="formPacientesAtendidosTecnologoModalidad" method="post" class="form-horizontal" >

        <div class="form-group col-sm-12">
            <label class="col-sm-2 control-label">Fecha Inicio</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fechaIni_9" name="fechaIni_9" readonly="readonly" />
            </div>
            <label class="col-sm-2 control-label">Fecha final</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="fechaFin_9" name="fechaFin_9" readonly="readonly" />
            </div>
        </div>
        <div class="form-group col-sm-12">
            <label class="col-sm-2 control-label">Modalidad</label>
            <div class="col-sm-6">
                <select id="modalidadRealizada_9" name="modalidadRealizada_9" class="form-control">
                    <option value=""></option>
                    <optgroup label="Imagenología">
                    {% for modalidad in modalidades %}
                        <option value="{{ modalidad.id }}">{{ modalidad }}</option>
                    {% endfor %}
                    </optgroup>
                </select>
            </div>
        </div>
        <div class="form-group col-sm-12">
            <label class="col-sm-2 control-label">Tecnólogo</label>
            <div class="col-sm-6">
                <select id="tecnologoAtendio_9" name="tecnologoAtendio_9" class="form-control">
                    <option value=""></option>
                    {% for tipo in tiposEmpleado if tipo.codigo == 'MED' or tipo.codigo == 'TRY' %}
                        <optgroup label="{{ tipo }}">
                        {% for tecnologo in tecnologos if tecnologo.idTipoEmpleado is not null and tecnologo.idTipoEmpleado.id == tipo.id %}
                            <option value="{{ tecnologo.id }}">{{ tecnologo }}</option>
                        {% endfor %}
                        </optgroup>
                    {% endfor %}
                    {% for tecnologo in tecnologos if tecnologo.idTipoEmpleado is null %}
                        <option value="{{ tecnologo.id }}">{{ tecnologo }}</option>
                    {% endfor %}
                </select>
            </div>
        </div>
        
        <input type="hidden" class="form-control" name="report_selector" value="9" />

    </form>