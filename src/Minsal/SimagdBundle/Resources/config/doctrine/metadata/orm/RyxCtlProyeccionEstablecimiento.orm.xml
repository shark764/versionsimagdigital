<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RyxCtlProyeccionEstablecimiento" table="ryx_ctl_proyeccion_establecimiento">
    <indexes>
      <index name="IDX_339A0DE0A7750BE9" columns="id_area_examen_estab"/>
      <index name="IDX_339A0DE0BE76A184" columns="id_proyeccion"/>
      <index name="IDX_339A0DE0AC39DE56" columns="id_user_mod"/>
      <index name="IDX_339A0DE0D8A5832B" columns="id_user_reg"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_proyeccion_area_examen_estab" columns="id_area_examen_estab,id_proyeccion"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <field name="fechaHoraMod" type="datetime" column="fecha_hora_mod" nullable="true"/>
    <field name="habilitado" type="boolean" column="habilitado" nullable="true"/>
    <field name="observaciones" type="string" column="observaciones" length="255" nullable="true"/>
    <many-to-one field="idUserReg" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserMod" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_mod" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProyeccion" target-entity="RyxCtlProyeccionRadiologica">
      <join-columns>
        <join-column name="id_proyeccion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAreaExamenEstab" target-entity="MntAreaExamenEstablecimiento">
      <join-columns>
        <join-column name="id_area_examen_estab" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
