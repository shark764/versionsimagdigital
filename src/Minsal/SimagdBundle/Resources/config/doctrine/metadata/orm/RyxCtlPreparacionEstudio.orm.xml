<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RyxCtlPreparacionEstudio" table="ryx_ctl_preparacion_estudio">
    <indexes>
      <index name="IDX_8BC10B6CEFA31A58" columns="id_area_servicio_diagnostico_aplica"/>
      <index name="IDX_8BC10B6C592B0EA1" columns="id_empleado_registra"/>
      <index name="IDX_8BC10B6C7DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_8BC10B6CAC39DE56" columns="id_user_mod"/>
      <index name="IDX_8BC10B6CD8A5832B" columns="id_user_reg"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_codigo_preparacion_estudio" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="preparacionEstudio" type="text" column="preparacion_estudio" nullable="true"/>
    <field name="recomendaciones" type="text" column="recomendaciones" nullable="true"/>
    <field name="observaciones" type="string" column="observaciones" length="255" nullable="true"/>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <field name="fechaHoraMod" type="datetime" column="fecha_hora_mod" nullable="true"/>
    <field name="nombre" type="string" column="nombre" length="150" nullable="true"/>
    <field name="codigo" type="string" column="codigo" nullable="true"/>
    <many-to-one field="idUserReg" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserMod" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_mod" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpleadoRegistra" target-entity="MntEmpleado">
      <join-columns>
        <join-column name="id_empleado_registra" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAreaServicioDiagnosticoAplica" target-entity="CtlAreaServicioDiagnostico">
      <join-columns>
        <join-column name="id_area_servicio_diagnostico_aplica" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
