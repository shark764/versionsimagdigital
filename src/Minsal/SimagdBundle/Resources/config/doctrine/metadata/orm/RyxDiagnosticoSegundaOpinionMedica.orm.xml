<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RyxDiagnosticoSegundaOpinionMedica" table="ryx_diagnostico_segunda_opinion_medica">
    <indexes>
      <index name="IDX_F185AA86838319BF" columns="id_diagnostico"/>
      <index name="IDX_F185AA86890253C7" columns="id_empleado"/>
      <index name="IDX_F185AA867DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_F185AA869FA5140" columns="id_tipo_nota_diagnostico"/>
      <index name="IDX_F185AA86D8A5832B" columns="id_user_reg"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="contenido" type="text" column="contenido" nullable="true"/>
    <field name="fechaEmision" type="datetime" column="fecha_emision" nullable="true"/>
    <field name="observaciones" type="string" column="observaciones" length="255" nullable="true"/>
    <many-to-one field="idUserReg" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipoNotaDiagnostico" target-entity="RyxCtlTipoOpinionMedicaDiagnostico">
      <join-columns>
        <join-column name="id_tipo_nota_diagnostico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDiagnostico" target-entity="RyxDiagnosticoRadiologico">
      <join-columns>
        <join-column name="id_diagnostico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
