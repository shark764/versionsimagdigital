<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RyxCtlProyeccionRadiologica" table="ryx_ctl_proyeccion_radiologica">
    <indexes>
      <index name="IDX_D4F3A48942479DDC" columns="id_examen_servicio_diagnostico"/>
      <index name="IDX_D4F3A489AC39DE56" columns="id_user_mod"/>
      <index name="IDX_D4F3A489D8A5832B" columns="id_user_reg"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_codigo_proyeccion" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false"/>
    <field name="codigo" type="string" column="codigo" nullable="true"/>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <field name="fechaHoraMod" type="datetime" column="fecha_hora_mod" nullable="true"/>
    <field name="tiempoOcupacionSala" type="smallint" column="tiempo_ocupacion_sala" nullable="true"/>
    <field name="tiempoMedico" type="smallint" column="tiempo_medico" nullable="true"/>
    <field name="descripcion" type="text" column="descripcion" nullable="true"/>
    <field name="observaciones" type="string" column="observaciones" length="255" nullable="true"/>
    <many-to-one field="idUserReg" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserMod" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_mod" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idExamenServicioDiagnostico" target-entity="CtlExamenServicioDiagnostico">
      <join-columns>
        <join-column name="id_examen_servicio_diagnostico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
