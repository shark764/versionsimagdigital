<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RyxProcedimientoMaterialUtilizado" table="ryx_procedimiento_material_utilizado">
    <indexes>
      <index name="IDX_5CD9169D2C659900" columns="id_material"/>
      <index name="IDX_5CD9169D9E9497EB" columns="id_procedimiento_realizado"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_material_procedimiento_realizado" columns="id_material,id_procedimiento_realizado"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="cantidadUtilizada" type="integer" column="cantidad_utilizada" nullable="true"/>
    <field name="otrasEspecificaciones" type="string" column="otras_especificaciones" length="100" nullable="true"/>
    <many-to-one field="idProcedimientoRealizado" target-entity="RyxProcedimientoRadiologicoRealizado">
      <join-columns>
        <join-column name="id_procedimiento_realizado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMaterial" target-entity="RyxCtlMaterial">
      <join-columns>
        <join-column name="id_material" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
