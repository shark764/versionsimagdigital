<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RyxCtlMaterialEstablecimiento" table="ryx_ctl_material_establecimiento">
    <indexes>
      <index name="IDX_C017481E7DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_C017481E2C659900" columns="id_material"/>
      <index name="IDX_C017481EAC39DE56" columns="id_user_mod"/>
      <index name="IDX_C017481ED8A5832B" columns="id_user_reg"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_material_establecimiento" columns="id_material,id_establecimiento"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="cantidadDisponible" type="integer" column="cantidad_disponible" nullable="false"/>
    <field name="descripcion" type="text" column="descripcion" nullable="true"/>
    <field name="habilitado" type="boolean" column="habilitado" nullable="true"/>
    <field name="fechaHoraReg" type="datetime" column="fecha_hora_reg" nullable="true"/>
    <field name="fechaHoraMod" type="datetime" column="fecha_hora_mod" nullable="true"/>
    <many-to-one field="idUserReg" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_reg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUserMod" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_user_mod" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMaterial" target-entity="RyxCtlMaterial">
      <join-columns>
        <join-column name="id_material" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
