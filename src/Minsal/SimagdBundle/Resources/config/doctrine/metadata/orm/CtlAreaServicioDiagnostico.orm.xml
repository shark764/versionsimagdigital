<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlAreaServicioDiagnostico" table="ctl_area_servicio_diagnostico">
    <indexes>
      <index name="IDX_C44C3A31695EA351" columns="id_atencion"/>
      <index name="IDX_C44C3A316724C8DC" columns="idusuariomod"/>
      <index name="IDX_C44C3A3113B895A1" columns="idusuarioreg"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="idarea" type="string" column="idarea" length="10" nullable="false"/>
    <field name="nombrearea" type="string" column="nombrearea" length="75" nullable="true"/>
    <field name="administrativa" type="string" column="administrativa" nullable="true"/>
    <field name="fechahorareg" type="datetime" column="fechahorareg" nullable="true"/>
    <field name="fechahoramod" type="datetime" column="fechahoramod" nullable="true"/>
    <field name="imgCodigo" type="string" column="img_codigo" nullable="true"/>
    <field name="imgDescripcion" type="text" column="img_descripcion" nullable="true"/>
    <field name="imgObservaciones" type="string" column="img_observaciones" length="255" nullable="true"/>
    <many-to-one field="idusuarioreg" target-entity="FosUserUser">
      <join-columns>
        <join-column name="idusuarioreg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idusuariomod" target-entity="FosUserUser">
      <join-columns>
        <join-column name="idusuariomod" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAtencion" target-entity="CtlAtencion">
      <join-columns>
        <join-column name="id_atencion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
