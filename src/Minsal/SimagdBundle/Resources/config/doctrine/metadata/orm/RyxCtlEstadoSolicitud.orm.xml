<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RyxCtlEstadoSolicitud" table="ryx_ctl_estado_solicitud">
    <unique-constraints>
      <unique-constraint name="idx_codigo_estado_solicitud" columns="codigo"/>
    </unique-constraints>
    <id name="id" type="smallint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombreEstado" type="string" column="nombre_estado" nullable="false"/>
    <field name="codigo" type="string" column="codigo" nullable="false"/>
    <field name="estiloPresentacion" type="string" column="estilo_presentacion" nullable="true"/>
    <field name="porcentajeAvance" type="smallint" column="porcentaje_avance" nullable="false"/>
  </entity>
</doctrine-mapping>
