<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ImgSolicitudEstudioProyeccion" table="img_solicitud_estudio_proyeccion">
    <indexes>
      <index name="IDX_3983AC98CA460C63" columns="id_proyeccion_solicitada"/>
      <index name="IDX_3983AC986AAA01BF" columns="id_solicitud_estudio"/>
    </indexes>
    <unique-constraints>
